<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GuideLinerLayout">
        <!-- 指示器的个数 -->
        <attr name="indicatorNumber" format="integer"></attr>
        <!-- 默认选中第几个 默认从0开始 -->
        <attr name="defaultSelectView" format="integer"></attr>
        <!-- 指示器的大小 默认30dp -->
        <attr name="indicatorWidth" format="integer"></attr>
        <attr name="indicatorHeight" format="integer"></attr>
        <!-- 指示器的间距 默认10 -->
        <attr name="indicatorMargin" format="integer"></attr>
    </declare-styleable>


</resources>